{
  "expo": {
    "name": "Agenda Saúde",
    "slug": "Agenda-Saude",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lukajlp.AgendaSaude",
      "infoPlist": {
        "NSCameraUsageDescription": "Este aplicativo precisa acessar sua câmera para tirar fotos.",
        "NSPhotoLibraryUsageDescription": "Este aplicativo precisa acessar sua galeria de fotos para selecionar imagens."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#58B678"
      },
      "package": "com.lukajlp.AgendaSaude",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "O aplicativo precisa acessar suas fotos para que você possa compartilhá-las.",
          "cameraPermission": "O aplicativo precisa acessar sua câmera para tirar fotos."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e7f01e73-d353-48b3-ab32-9b57a08ddbb6"
      }
    }
  }
}
